{
  "editor.fontFamily": "'cascadia code',Consolas, 'Courier New', monospace",
  "editor.guides.bracketPairs": "active",
  "editor.fontSize": 18,
  "dart.previewFlutterUiGuides": true,
  "workbench.iconTheme": "material-icon-theme",
  "terminal.integrated.defaultProfile.windows": "Command Prompt",
  "dart.previewHotReloadOnSaveWatcher": true,
  "dart.hotReloadOnSave": "all",
  // "workbench.list.multiSelectModifier": "alt",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "explorer.confirmDelete": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
    },
    "editor.selectionHighlight": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.wordBasedSuggestions": "allDocuments",
    "editor.foldingImportsByDefault": true,
    "editor.acceptSuggestionOnCommitCharacter": true
    },
  "update.enableWindowsBackgroundUpdates": false,
  "update.mode": "manual",
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "workbench.editorAssociations": {
    "*.svg": "default"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDragAndDrop": false,
  "workbench.startupEditor": "none",
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "editor.inlineSuggest.enabled": true,
  "dart.debugExternalPackageLibraries": false,
  "dart.debugSdkLibraries": false,


  "workbench.colorCustomizations": {
    "tab.activeBorder": "#FFCB6B",
    "tab.activeBorderTop": "#FFCB6B",
    "tab.hoverBackground": "#ffffff32",
    "tab.activeBackground": "#00000084",
    "terminalCursor.foreground": "#FFCB6B",
    "terminalCursor.background": "#000000",
    "editorCursor.foreground": "#FFCC00",
    "scrollbar.shadow": "#0F111A00",
    "editor.findMatchBorder": "#FFCC00",
    "editor.multiCursorPaste": "full",
    "editor.cursorBlinking": "smooth",
    "editor.cursorWidth": 3,
    "editor.findMatchBackground": "#08080800",
    "editor.findMatchHighlightBorder": "#ffffff7b",
    "editor.findMatchHighlightBackground": "#3481d971",
    "editor.inactiveSelectionBackground": "#4f4e4e37",
    "searchEditor.findMatchBackground": "#08080800",
    "searchEditor.findMatchBorder": "#ffffff7b",
    "editor.lineHighlightBorder": "#ffcc0034",
    "editor.wordHighlightBackground": "#00000000",
    "statusBar.background": "#1458a5f0",
    "statusBar.foreground": "#ffffff",
    // "editor.lineHighlightBackground": "#625a5a5a",
    "badge.background": "#ffcc00ea",
    "badge.foreground": "#000000",
    // "activityBarBadge.background": "#ffcc00ea",
    // "activityBarBadge.foreground": "#000000",
    "activityBar.activeBackground": "#4c82c164",
    "activityBar.activeBorder": "#0091ff",
    "activityBar.border": "#fffefe3b",
    // "button.border": "#FFCC00",
    // "dart.flutterUiGuides": "#ffffffcc",
    "editor.hoverHighlightBackground": "#ffffff20",
    "menubar.selectionBackground": "#1379edab",
    "menubar.selectionForeground": "#ffffff",
  },
  "outline.problems.colors": true,
  "javascript.inlayHints.enumMemberValues.enabled": true,
  "explorer.openEditors.minVisible": 100,
  "tabnine.experimentalAutoImports": true,
  "workbench.sideBar.location": "right",
  "editor.defaultFormatter": "Dart-Code.dart-code",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "thunder-client.saveTokenPerEnvironment": true,
  "thunder-client.customLocation": "C:\\Users\\devar\\.vscode",
  "editor.accessibilitySupport": "off",
  "window.commandCenter": false,
  "thunder-client.httpLibrary": "axios",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "dart.lineLength": 150,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "javascript.validate.enable": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "debug.allowBreakpointsEverywhere": true,
  "workbench.colorTheme": "Default Dark+",
  "terminal.integrated.enableMultiLinePasteWarning": "always",
  "gitlens.launchpad.indicator.enabled": false,
  "gitlens.launchpad.indicator.polling.enabled": false,
"files.autoSave": "afterDelay",
}
